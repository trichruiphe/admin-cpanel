                                        <div class="ut-font-w500">S,M,XL</div>
                                    </div>
                                    <div class="col ut-text-right">
                                        <div class="ut-small-fz">Sold</div>
                                        <div class="ut-font-w500">108</div>
                                    </div>
                                    <div class="w-100 ut-dis-updo"></div>
                                    <div class="col">
                                        <div class="ut-small-fz">Colors</div>
                                        <span class="ut-circle-color ut-circle-black"></span><span class="ut-circle-color ut-circle-red"></span>
                                    </div>
                                    <div class="col ut-text-right">
                                        <div class="ut-small-fz">Price</div>
                                        <div class="ut-font-w500">150.000đ</div>
                                    </div>
                                    <div class="w-100 ut-dis-updo"></div>
                                    <div class="col">
                                        <div class="ut-small-fz">Code</div>
                                        <div class="ut-font-w500">#843587</div>
                                    </div>
                                </div>
                            </div>
                        </a>
                    </div>
                    <% } %>
                </div>
            </div>
            <div class="card-footer">tung</div>
        </div>
